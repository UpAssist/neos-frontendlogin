prototype(UpAssist.Neos.FrontendLogin:RegistrationForm) < prototype(TYPO3.Neos:Plugin) {
    templatePath = 'resource://UpAssist.Neos.FrontendLogin/Private/Templates/PluginTypes/User/New.html'
    package = 'UpAssist.Neos.FrontendLogin'
    controller = 'User'
    action = 'new'
    redirectNode = TYPO3.Neos:NodeUri {
        node = ${q(node).property('redirectNode')}
    }
    logoutRedirectNode = TYPO3.Neos:NodeUri {
        node = ${q(node).property('logoutRedirectNode')}
    }
}